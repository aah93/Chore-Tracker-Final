


<% loginstatus = getSubLoggedUser() %>
<% if(!loginstatus or loginstatus[:type] != "parent") %>
   <h2>You must be logged in as a parent to use this page.</h2>
   <h2>You are being redirected...</h2>
   <script type="text/javascript">
     window.location.href="/sublogin/?type=parent"  // put your correct path in a string here
   </script>
<% else %>
    <h1>New Chore</h1>
    <%= render 'form', chore: @chore%>
    <%= link_to 'Back', chores_path %>
<% end %>